{% from "head.jinja" import head %}
{% from "scripts.jinja" import scripts %}
{% from "navbar.jinja" import navbar %}
{% from "footer.jinja" import footer %}
{% from "projectHero.jinja" import projectHero %}
{% from "projectOverviewAlt.jinja" import projectOverviewAlt %}
{% from "projectPagination.jinja" import projectPagination %}

<!DOCTYPE html>
<html lang="en">
  {{ head(projectData) }}

  <body>
    {{ navbar() }}

    <main>
      {{ projectHero(projectData) }}

      {{ projectOverviewAlt(projectData) }}

      <div class="container">
        <div class="row">
          <div class="project-text">
            <h2>Newsletter</h2>
            <p>Sent out bi-weekly emails, sometimes polling users and used the data for articles, social posts, and the following newsletter.</p>
          </div>
        </div>
      </div>

      <div class="container-xxl">
        <div class="row">
          <div class="col">
            <div class="row mb-0 justify-content-center">
              <div class="col-12 col-sm-6 col-md">
                <img src="../static/images/work/khealth/infographics/newsletter-sex-survey.png" class="figure-img img-fluid" alt="Interactive services page">
              </div>
              <div class="col-12 col-md-6">
                <img src="../static/images/work/khealth/infographics/typeform-sex-survey.png" class="figure-img img-fluid" alt="Portrait services page">
              </div>
              <div class="col-12 col-sm-6 col-md">
                <img src="../static/images/work/khealth/infographics/newsletter-sex-results.png" class="figure-img img-fluid" alt="List of services page">
              </div>
            </div>
            <!-- <figcaption class="figure-caption">
            </figcaption> -->
          </div>
        </div>
      </div>

      <div class="container-xxl">
        <div class="row">
          <div class="col">
            <figure class="figure">
              <img src="../static/images/work/khealth/infographics/social-sex-survey.png" class="figure-img img-fluid" alt="Intro of the Pretty Instant brand book">
            </figure>
            <!-- <figcaption class="figure-caption">
            </figcaption> -->
          </div>
        </div>
      </div>

      <div class="container-xxl">
        <div class="row">
          <div class="col">
            <div class="row mb-0 justify-content-center">
              <div class="col-12 col-sm-6 col-lg">
                <img src="../static/images/work/khealth/infographics/newsletter-insomnia.png" class="figure-img img-fluid" alt="Interactive services page">
              </div>
              <div class="col-12 col-sm-6 col-lg">
                <img src="../static/images/work/khealth/infographics/blog-insomnia-heatmap.png" class="figure-img img-fluid rounded" alt="Interactive services page">
              </div>
              <div class="col-12 col-sm-6 col-lg">
                <img src="../static/images/work/khealth/infographics/blog-insomnia-timeline.png" class="figure-img img-fluid rounded" alt="Portrait services page">
              </div>
              <div class="col-12 col-sm-6 col-lg">
                <img src="../static/images/work/khealth/infographics/blog-insomnia-conditions.png" class="figure-img img-fluid rounded" alt="List of services page">
              </div>
            </div>
            <!-- <figcaption class="figure-caption">
            </figcaption> -->
          </div>
        </div>
      </div>

      <div class="container">
        <div class="row">
          <div class="project-text">
            <h2>Social media posts</h2>
            <!-- <p>Social media posts</p> -->
          </div>
        </div>
      </div>

      <div class="container-xxl">
        <div class="row mb-0">
          <div class="col">
            <div class="row mb-0 justify-content-center">
              <div class="col-12 col-md-8">
                <img src="../static/images/work/khealth/infographics/social-urgent-care.png" class="figure-img img-fluid" alt="Portrait services page">
              </div>
              <div class="col-12 col-sm-6 col-md">
                <img src="../static/images/work/khealth/infographics/social-symptoms.png" class="figure-img img-fluid" alt="List of services page">
              </div>
            </div>
            <!-- <figcaption class="figure-caption">
            </figcaption> -->
          </div>
        </div>
      </div>

      {{ projectPagination(projectData, previousSlug="k-health-emails", previousTitle="Emails", previousCompany="K Health", nextSlug="k-health-social", nextTitle="Social", nextCompany="K Health") }}

    </main>

    {{ footer() }}

    {{ scripts() }}
  </body>
</html>