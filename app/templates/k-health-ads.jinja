{% from "head.jinja" import head %}
{% from "scripts.jinja" import scripts %}
{% from "navbar.jinja" import navbar %}
{% from "footer.jinja" import footer %}
{% from "projectHero.jinja" import projectHero %}
{% from "projectOverviewAlt.jinja" import projectOverviewAlt %}
{% from "projectPagination.jinja" import projectPagination %}

<!DOCTYPE html>
<html lang="en">
	{{ head(projectData) }}

  <body>
    {{ navbar() }}

    <main>
      {{ projectHero(projectData) }}

      {{ projectOverviewAlt(projectData) }}

      <div class="container">
        <div class="row mb-7">
          <div class="project-text">
            <h2>12 second ads</h2>
            <p>Showcasing the 6 most popular medications available through K Health</p>
          </div>
        </div>
      </div>

      <div class="container-xxl">
        <div class="row">
          <div class="col-12 col-md-6 col-xl-4 mb-3">
            <figure class="figure">
              <img src="../static/images/work/khealth/ads/mh-bupropion.png" class="figure-img img-fluid" alt="Intro of the Pretty Instant brand book">
            </figure>
          </div>
          <div class="col-12 col-md-6 col-xl-4 mb-3">
            <figure class="figure">
              <img src="../static/images/work/khealth/ads/mh-buspirone.png" class="figure-img img-fluid" alt="Intro of the Pretty Instant brand book">
            </figure>
          </div>
          <div class="col-12 col-md-6 col-xl-4 mb-3">
            <figure class="figure">
              <img src="../static/images/work/khealth/ads/mh-sertraline-50.png" class="figure-img img-fluid" alt="Intro of the Pretty Instant brand book">
            </figure>
          </div>
          <div class="col-12 col-md-6 col-xl-4 mb-3 mb-xl-0">
            <figure class="figure">
              <img src="../static/images/work/khealth/ads/mh-sertraline-100.png" class="figure-img img-fluid" alt="Intro of the Pretty Instant brand book">
            </figure>
          </div>
          <div class="col-12 col-md-6 col-xl-4 mb-3 mb-md-0">
            <figure class="figure">
              <img src="../static/images/work/khealth/ads/mh-fluoxetine.png" class="figure-img img-fluid" alt="Intro of the Pretty Instant brand book">
            </figure>
          </div>
          <div class="col-12 col-md-6 col-xl-4">
            <figure class="figure">
              <img src="../static/images/work/khealth/ads/mh-escitalopram.png" class="figure-img img-fluid" alt="Intro of the Pretty Instant brand book">
            </figure>
          </div>
          <!-- <figcaption class="figure-caption">
          </figcaption> -->
        </div>

        <div class="container">
          <div class="row mb-7">
            <div class="project-text">
              <h2>30 second ads</h2>
              <p>Storyboard of an ad that highlights the affordability of K Health's Mental Health program</p>
            </div>
          </div>
        </div>

        <div class="row">
          <div class="col">
            <figure class="figure">
              <img src="../static/images/work/khealth/ads/mh-storyboard.png" class="figure-img img-fluid" alt="Intro of the Pretty Instant brand book">
            </figure>
          </div>
          <!-- <figcaption class="figure-caption">
          </figcaption> -->
        </div>
      </div>

      {{ projectPagination(projectData, previousSlug="k-health-social", previousTitle="Social", previousCompany="K Health", nextSlug="pretty-instant-website", nextTitle="Website redesign", nextCompany="Pretty Instant") }}

    </main>

    {{ footer() }}

		{{ scripts() }}
	</body>
</html>